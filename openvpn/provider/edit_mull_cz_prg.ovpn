client
dev tun
resolv-retry infinite
nobind
persist-key
persist-tun
verb 3
remote-cert-tls server
ping 10
ping-restart 60
sndbuf 524288
rcvbuf 524288
cipher AES-256-GCM
tls-cipher TLS-DHE-RSA-WITH-AES-256-GCM-SHA384
auth-user-pass mullvad_userpass.txt
ca mullvad_ca.crt
script-security 2
fast-io
remote-random

# Force TCP on port 443 with known Mullvad Prague TCP-capable endpoints
remote cz-prg-ovpn-101.relays.mullvad.net 443 tcp
remote cz-prg-ovpn-102.relays.mullvad.net 443 tcp
